{
  "name": "@starladder/ui-react",
  "version": "1.3.16",
  "description": "Components library for react projects",
  "main": "dist/main.js",
  "scripts": {
    "pre-commit": "lint-staged",
    "format": "prettier --write \"src/**/*.jsx\" \"src/**/*.js\" \"src/**/*.css\"",
    "lint": "eslint -c .eslintrc src/**/*.js && eslint -c .eslintrc src/**/*.jsx && stylelint src/**/*.css",
    "flow": "flow",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "yarn webpack",
    "prepublishOnly": "yarn lint",
    "prepare": "yarn build",
    "preversion": "yarn format && yarn lint",
    "version": "git add --all",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starladder/ui-react.git"
  },
  "keywords": [
    "react",
    "ui-react",
    "react-components"
  ],
  "author": "kris2519",
  "contributors": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/starladder/ui-react/issues"
  },
  "homepage": "https://github.com/starladder/ui-react#readme",
  "peerDependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^2.1.2",
    "flow-bin": "^0.102.0",
    "html-loader": "^0.5.5",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "react-animate-height": "^2.0.15",
    "react-modal": "^3.10.1",
    "react-paginate": "^6.3.0",
    "storybook-readme": "^5.0.8",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "svg-spritemap-webpack-plugin": "^3.3.5",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write src/**/*.{js, jsx}",
      "yarn eslint --c .eslintrc"
    ],
    "*.css": [
      "prettier --write src/**/*.css",
      "stylelint"
    ]
  }
}
